# Database Configuration
DATABASE_URL="postgresql://username:password@localhost:5432/aethermind?schema=public"

# JWT Configuration
JWT_SECRET="CHANGE-THIS-TO-A-SECURE-RANDOM-STRING-MIN-32-CHARACTERS"
JWT_EXPIRES_IN="7d"

# Bcrypt Configuration
BCRYPT_ROUNDS=10

# Google OAuth (Get from: https://console.cloud.google.com/)
GOOGLE_CLIENT_ID="your-google-client-id.apps.googleusercontent.com"
GOOGLE_CLIENT_SECRET="your-google-client-secret"
GOOGLE_REDIRECT_URI="https://aethermindapi-production.up.railway.app/api/auth/callback/google"

# GitHub OAuth (Get from: https://github.com/settings/developers)
GITHUB_CLIENT_ID="your-github-client-id"
GITHUB_CLIENT_SECRET="your-github-client-secret"
GITHUB_REDIRECT_URI="https://aethermindapi-production.up.railway.app/api/auth/callback/github"

# Frontend URL (for OAuth redirects)
FRONTEND_URL="https://aethermind-page.vercel.app"

# Node Environment
NODE_ENV="development"

# API Configuration (existing)
PORT=3000
API_KEY_HASH=""

# Redis (existing)
REDIS_URL="redis://localhost:6379"

# AI Provider Keys (existing)
OPENAI_API_KEY=""
ANTHROPIC_API_KEY=""

# CORS Origins (existing)
CORS_ORIGINS="http://localhost:3000,http://localhost:3001"

# Rate Limiting (existing)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Request Body Limit (existing)
REQUEST_BODY_LIMIT="10mb"
